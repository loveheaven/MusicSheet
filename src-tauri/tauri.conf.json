{
  "productName": "Music Sheet Reader",
  "version": "1.0.0",
  "identifier": "com.musicsheet.reader",
  "build": {
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build",
    "devUrl": "http://localhost:1423",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "Music Sheet Reader",
        "width": 1200,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "minWidth": 800,
        "minHeight": 600,
        "center": true,
        "visible": true,
        "decorations": true,
        "alwaysOnTop": false,
        "skipTaskbar": false
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  }
}